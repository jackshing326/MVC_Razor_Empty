  <data name="Select_Administrator" xml:space="preserve">
    <value>SELECT [Administrator] as CityName FROM [Administration_SchoolCode] group by [Administrator]</value>
  </data>
  <data name="Select_CityName" xml:space="preserve">
    <value>SELECT CityName  FROM [JuniorSchoolZipCode] group by CityName order by CityName</value>
  </data>
  <data name="Select_IDandPW" xml:space="preserve">
    <value>SELECT US_ID FROM [USERS] where US_ID='{0}' and US_PW='{1}'</value>
    <comment>驗證帳密</comment>
  </data>
  <data name="Select_Junior" xml:space="preserve">
    <value>SELECT * FROM [RankDistribution_Junior_Table] where TestYear='{0}' and SchoolCode='{1}'</value>
  </data>
  <data name="Select_JuniorSchoolName" xml:space="preserve">
    <value>SELECT Rtrim([SchoolCode])+','+Rtrim([SchoolName]) as SchoolName FROM [JuniorSchoolZipCode]  where [SchoolCode] in (select [SchoolCode] from SchoolCodeAnalysis where TestYear='{0}' and  CityName='{1}')</value>
  </data>
  <data name="Select_Senior" xml:space="preserve">
    <value>SELECT * FROM  [RankDistribution_Senior_Table]  where TestYear='{0}' and  SchoolCode='{1}' and DeptCode='{2}'</value>
  </data>
  <data name="Select_SeniorSchoolName" xml:space="preserve">
    <value>SELECT AdsDep.CombineName as SchoolCode,(Rtrim(AdS.[SchoolName])+','+AdsDep.CombineName) as SchoolName FROM [Administration_SchoolCode] AdS left join (SELECT [ADMSchoolCode],  [DeptCode] , Rtrim([ADMSchoolCode])+'_'+Rtrim([DeptCode]) as CombineName FROM [SeniorStudentData] where TestYear='{0}' group by [ADMSchoolCode],[DeptCode]) AdsDep on AdS.[SchoolCode]=AdsDep.ADMSchoolCode where Ads.Administrator='五專' and AdsDep.CombineName is not null order by AdS.[SchoolCode]</value>
  </data>
  <data name="Select_Setting" xml:space="preserve">
    <value>SELECT [ItemNo],[ReportKind],[RptFileName],[RptFilePath],[RptVariables],[RptVariablesName],[OutPutPath],[OutPutFileName],[SPName_Query],[SPName_QueryCSV],[PDFFile],[CSVFile],[Check_Data_SP],[Check_Data_SP_Variable] FROM [RankDistribution].[dbo].[Setting]</value>
